h > upperPageBound){\r\n            pageIncrementBtn = <li className={styles.paginationbutton}><a href='#' onClick={this.btnIncrementClick}> &hellip; </a></li>\r\n        }\r\n        let pageDecrementBtn = null;\r\n        if(lowerPageBound >= 1){\r\n            pageDecrementBtn = <li className={styles.paginationbutton}><a href='#' onClick={this.btnDecrementClick}> &hellip; </a></li>\r\n        }\r\n        let renderPrevBtn = null;\r\n        if(isPrevBtnActive === 'disabled') {\r\n            renderPrevBtn = <li className={`{pagination-button pagination-button-prev ${isPrevBtnActive}}`}><span id=\"btnPrev\"> {`<`} </span></li>\r\n        }\r\n        else{\r\n            renderPrevBtn = <li className={`pagination-button pagination-button-prev ${isPrevBtnActive}`}><a href='#' id=\"btnPrev\" onClick={this.btnPrevClick}> {`<`} </a></li>\r\n        }\r\n        let renderNextBtn = null;\r\n        if(isNextBtnActive === 'disabled') {\r\n            renderNextBtn = <li className={`pagination-button pagination-button-next ${isNextBtnActive}`}><span id=\"btnNext\"> {`>`} </span></li>\r\n        }\r\n        else{\r\n            renderNextBtn = <li className={`pagination-button pagination-button-next ${isNextBtnActive}`}><a href='#' id=\"btnNext\" onClick={this.btnNextClick}> {`>`} </a></li>\r\n        }\r\n\r\n        return (\r\n            <div>\r\n                <div className={styles.contentdoubleone} className={styles.resourcescontainer}>\r\n                    {renderTodos}\r\n                </div>\r\n                <div className={styles.pagination}>\r\n                    {renderPrevBtn}\r\n                    {pageDecrementBtn}\r\n                    {renderPageNumbers}\r\n                    {pageIncrementBtn}\r\n                    {renderNextBtn}\r\n                </div>\r\n            </div>\r\n        )\r\n    }\r\n};\r\n\r\nexport default ResourcesItem;"]},"metadata":{},"sourceType":"module"}